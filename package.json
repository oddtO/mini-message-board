    "express": "^4.19.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.10.1",
    "@eslint/js": "^9.8.0",
    "@html-eslint/eslint-plugin": "^0.26.0",
    "@html-eslint/parser": "^0.26.0",
    "@pgtyped/cli": "^2.3.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.8.0",
    "gen-env-types": "^1.3.4",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "pgtyped": "^0.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-embed": "^0.4.15",
    "prettier-plugin-sql": "^0.18.1",
    "sass": "^1.77.8",
    "sql-lint": "^1.0.0",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.5.0",
